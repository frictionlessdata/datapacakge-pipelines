            "path": "characters.csv",
            "schema": {
                "fields": [
                    {"name": "first_name", "type": "string"},
                    {"name": "last_name", "type": "string"},
                    {"name": "house", "type": "string"},
                    {"name": "age", "type": "number", "units": "Westerosian Years"}
                ]
            }
        },
        {
            "name": "got-houses",
            "dpp:streaming": true,
            "path": "houses.csv",
            "schema": { "fields": [
                {"name": "house", "type": "string"}
            ]}
        }
    ]
}
